<Window x:Class="PL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,0,0,-6">
        <Grid.Background>
            <ImageBrush ImageSource="/DeliveryDrones.jpg"/>
        </Grid.Background>
        <Button x:Name="ShowDronesButton" Content="Drones List" HorizontalAlignment="Center" Margin="0,345,0,0" VerticalAlignment="Top" Height="54" Width="132" Click="ShowDronesButton_Click" Background="#FFD9D9D9" BorderBrush="White" FontWeight="Bold" FontSize="16"/>
        <Label x:Name="WingsTitleLabel" Content="WINGS" HorizontalAlignment="Left" Height="121" Margin="315,156,0,0" VerticalAlignment="Top" Width="211" Background="{x:Null}" FontWeight="Bold" FontSize="48" RenderTransformOrigin="0.5,0.5" FontFamily="Segoe Script">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.063"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>

    </Grid>
</Window>
